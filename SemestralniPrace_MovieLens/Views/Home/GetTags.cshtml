@model IEnumerable<SemestralniPrace_MovieLens.Models.Tags>
@if (@Model.Count() != 0)
{
    <span id="loaded">
    @foreach (var item in Model)
    {
        <span class="count">@item.UserId (@item.Timestamp): <span style="font-weight:bold">#@item.Tag</span>;</span>
    }
    </span>  
}
else { 
    <span>No tags found!</span>
}